$JQuery("#feed_container").prepend("<%= escape_javascript(render 'list') %>");
<% @statuses.each do |status| %>
	$JQuery("#status_<%= status.id %>").effect("highlight", {}, 3000);
	$JQuery("#status_<%= status.id %> .timeago").timeago();
<% end %>
$JQuery(".flash_notice").remove();
$JQuery(".notice").remove();
